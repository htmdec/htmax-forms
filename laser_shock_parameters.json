{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Laser Shock Experiment Parameters",
    "type": "object",
    "properties": {
        "Timestamp": {
            "type": "string",
            "format": "date-time",
            "title": "Timestamp",
            "propertyOrder": 1
        },
        "Exp_ID": {
            "type": "integer",
            "title": "Exp_ID",
            "propertyOrder": 2
        },
        "Flyer_ID": {
            "type": "string",
            "title": "Flyer_ID",
            "propertyOrder": 3
        },
        "Flyer_material": {
            "type": "string",
            "title": "Flyer_material",
            "propertyOrder": 4
        },
        "Flyer_Thickness_um": {
            "type": "integer",
            "title": "Flyer_Thickness (um)",
            "propertyOrder": 5
        },
        "Sample_ID": {
            "type": "string",
            "title": "Sample_ID",
            "propertyOrder": 6
        },
        "Sample_material": {
            "type": "string",
            "title": "Sample material",
            "propertyOrder": 7
        },
        "Spacing_um": {
            "type": "integer",
            "title": "Spacing (um)",
            "propertyOrder": 8
        },
        "Waveplate_Angle_Degrees": {
            "type": "integer",
            "title": "Waveplate_Angle (Degrees)",
            "propertyOrder": 9
        },
        "PDV_FileName": {
            "type": "string",
            "title": "PDV_FileName",
            "propertyOrder": 10
        },
        "PDV_Target_Wavelength_m": {
            "type": "number",
            "title": "PDV_Target_Wavelength (m)",
            "propertyOrder": 11
        },
        "PDV_Target_Power_dBm": {
            "type": "number",
            "title": "PDV_Target_Power (dBm)",
            "propertyOrder": 12
        },
        "PDV_Ref_Wavelength_m": {
            "type": "number",
            "title": "PDV_Ref_Wavelength (m)",
            "propertyOrder": 13
        },
        "PDV_Ref_Power_dBm": {
            "type": "number",
            "title": "PDV_Ref_Power (dBm)",
            "propertyOrder": 14
        },
        "PDV_Return_Power_dBm": {
            "type": "number",
            "title": "PDV_Return_Power (dBm)",
            "propertyOrder": 15
        },
        "Flyer_Row": {
            "type": "integer",
            "title": "Flyer_Row",
            "propertyOrder": 16
        },
        "Flyer_Column": {
            "type": "integer",
            "title": "Flyer_Column",
            "propertyOrder": 17
        },
        "Flyer_X_Position_Desired_mm": {
            "type": "number",
            "title": "Flyer_X_Position_Desired (mm)",
            "propertyOrder": 18
        },
        "Flyer_Y_Position_Desired_mm": {
            "type": "number",
            "title": "Flyer_Y_Position_Desired (mm)",
            "propertyOrder": 19
        },
        "Flyer_X_Position_Corrected_mm": {
            "type": "number",
            "title": "Flyer_X_Position_Corrected (mm)",
            "propertyOrder": 20
        },
        "Flyer_Y_Position_Corrected_mm": {
            "type": "number",
            "title": "Flyer_Y_Position_Corrected (mm)",
            "propertyOrder": 21
        },
        "Laser_Ref_Energy_mJ": {
            "type": "number",
            "title": "Laser_Ref_Energy (mJ)",
            "propertyOrder": 22
        },
        "Laser_Target_Energy_mJ": {
            "type": "number",
            "title": "Laser_Target_Energy (mJ)",
            "propertyOrder": 23
        },
        "Beam_Profile_FileName": {
            "type": "string",
            "title": "Beam_Profile_FileName",
            "propertyOrder": 24
        },
        "Shot_Time_seconds": {
            "type": "number",
            "title": "Shot_Time (seconds)",
            "propertyOrder": 25
        },
        "Notes": {
            "type": "string",
            "title": "Notes",
            "propertyOrder": 26
        },
        "assignedIGSN": {
            "$ref": "#/$defs/assignedIGSN"
        },
        "name": {
            "$ref": "#/$defs/name"
        }
    },
    "required": [
        "Timestamp",
        "Exp_ID",
        "Flyer_ID",
        "Flyer_material",
        "Flyer_Thickness_um",
        "Sample_ID",
        "Sample_material",
        "Spacing_um",
        "Waveplate_Angle_Degrees",
        "PDV_FileName",
        "PDV_Target_Wavelength_m",
        "PDV_Target_Power_dBm",
        "PDV_Ref_Wavelength_m",
        "PDV_Ref_Power_dBm",
        "PDV_Return_Power_dBm",
        "Flyer_Row",
        "Flyer_Column",
        "Flyer_X_Position_Desired_mm",
        "Flyer_Y_Position_Desired_mm",
        "Flyer_X_Position_Corrected_mm",
        "Flyer_Y_Position_Corrected_mm",
        "Laser_Ref_Energy_mJ",
        "Laser_Target_Energy_mJ",
        "Beam_Profile_FileName",
        "Shot_Time_seconds",
        "Notes",
        "name",
        "assignedIGSN"
    ],
    "additionalProperties": false,
    "$defs": {
        "name": {
            "type": "string",
            "description": "Mirrors PDV_FileName",
            "title": "name",
            "watch": {
                "PDV_FileName": "PDV_FileName"
            },
            "options": {
                "hidden": true
            },
            "template": "{{PDV_FileName}}"
        },
        "assignedIGSN": {
            "type": "string",
            "title": "assignedIGSN",
            "watch": {
                "Sample_ID": "Sample_ID"
            },
            "options": {
                "hidden": true
            }
        }
    }
}